{
  "type": "module",
  "name": "@hazae41/argon2",
  "version": "1.0.22",
  "description": "Argon2 adapter for WebAssembly and JS implementations",
  "repository": "github:hazae41/argon2",
  "author": "hazae41",
  "scripts": {
    "examine": "deno lint ./src && deno check ./src && deno test ./src",
    "version": "deno -RW ./x.vsync.ts && git add deno.json",
    "prepare": "deno -RW ./x.dsync.ts && deno install",
    "prepack": "rm -rf ./out && tsc && tscousin"
  },
  "files": [
    "./out"
  ],
  "exports": {
    ".": {
      "types": "./out/mod.d.ts",
      "import": "./out/mod.js"
    }
  },
  "devDependencies": {
    "@hazae41/argon2-wasm": "^1.0.6",
    "@hazae41/phobos": "^2.0.13",
    "@hazae41/symbol-dispose-polyfill": "^1.0.2",
    "@hazae41/tscousin": "^1.0.28",
    "@noble/hashes": "^1.8.0",
    "typescript": "^5.9.3"
  },
  "keywords": [
    "argon2",
    "crypto",
    "cryptography",
    "adapter",
    "typescript",
    "esmodules"
  ],
  "dependencies": {
    "@hazae41/result-and-option": "^2.0.10"
  }
}
